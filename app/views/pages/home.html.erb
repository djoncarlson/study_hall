<% if signed_in? %>
  <table class="front" summary="For signed-in users">
    <tr>
      <td class="main">
        <h1 class="student">Assign Study Hall</h1>
        <%= render 'shared/student_form' %>
        <%= render 'shared/feed' %>
      </td>
    </tr>
  </table>
<% else %>
  <h1>Academia Cotopaxi Study Hall Manager</h1> 
  <%= link_to "Sign up", signup_path, :class => "signup_button round" %>
<% end %>
